<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name = "Regression Suite">
    <test name="app testing all iphone">
      <groups>
          <run>
              <include name="iphone"></include>
          </run>
      </groups>
        <classes>
        <class name="main.java.TestNGclassess.TNG_Group"></class>
        </classes>
    </test>
    <test name="app testing all samsung">
        <groups>
            <run>
                <include name="android"></include>
            </run>
        </groups>
        <classes>
            <class name="main.java.TestNGclassess.TNG_Group"></class>
        </classes>
    </test>
    <test name="app testing all huawei" enabled="false">
        <groups>
            <run>
                <include name="huawei"></include>
            </run>
        </groups>
        <classes>
            <class name="main.java.TestNGclassess.TNG_Group"></class>
        </classes>
    </test>
</suite>